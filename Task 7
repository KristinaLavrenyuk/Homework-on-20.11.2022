
#include <iostream>
#include <string>
#include <iomanip>
using namespace std;

int main()
{
    double nums = 0, j = 1;
    string s, letter;
    cin >> s;

    for (int i = 0; i < s.length(); i++)
    {
        switch (s[i])
        {
            case '1':
                nums += 1 * j;
                j *= 10;
                break;
            case '2':
                nums += 2 * j;
                j *= 10;
                break;
            case '3':
                nums += 3 * j;
                j *= 10;
                break;
            case '4':
                nums += 4 * j;
                j *= 10;
                break;
            case '5':
                nums += 5 * j;
                j *= 10;
                break;
            case '6':
                nums += 6 * j;
                j *= 10;
                break;
            case '7':
                nums += 7 * j;
                j *= 10;
                break;
            case '8':
                nums += 8 * j;
                j *= 10;
                break;
            case '9':
                nums += 9 * j;
                j *= 10;
                break;
            case '0':
                nums += 0 * j;
                j *= 10;
                break;
            default:
                letter += s[i];
        }
    }
    cout << letter << endl;
    cout << fixed << setprecision(0) << nums << endl;
    system("pause");
}
