
#include <iostream>
#include <string>
using namespace std;

int main()
{
    int nums[100]{}, j = 0;
    string s, letter;
    cin >> s;

    for (int i = 0; i < s.length(); i++)
    {
        switch (s[i])
        {
            case '1':
                nums[j] = 1;
                j++;
                break;
            case '2':
                nums[j] = 2;
                j++;
                break;
            case '3':
                nums[j] = 3;
                j++;
                break;
            case '4':
                nums[j] = 4;
                j++;
                break;
            case '5':
                nums[j] = 5;
                j++;
                break;
            case '6':
                nums[j] = 6;
                j++;
                break;
            case '7':
                nums[j] = 7;
                j++;
                break;
            case '8':
                nums[j] = 8;
                j++;
                break;
            case '9':
                nums[j] = 9;
                j++;
                break;
            case '0':
                nums[j] = 0;
                j++;
                break;
            default:
                letter += s[i];
        }
    }
    cout << letter << endl;
    for (int i = s.length() - letter.length() - 1; i >= 0; i--)
    {
        cout << nums[i];
    }
    system("pause");
}
